    "sass": "sass --style expanded src/scss/:dist/css/ --watch"
  },
  "dependencies": {
    "astro": "^2.1.7",
    "bootstrap": "^5.3.0-alpha2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "postcss": "^8.4.21",
    "sass": "^1.60.0"
  }
